<h1>Post List</h1>

<table>
  <thead>
  <td>title</td>
  <td>content</td>
  <td>action</td>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td>
        <%= link_to :edit, edit_post_path(post)%>
        <%= link_to :show, post_path(post) %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to :new, new_post_path %>